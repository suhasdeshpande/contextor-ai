{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "test": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "test:coverage": {
            "dependsOn": ["^build"],
            "outputs": ["coverage/**"]
        },
        "lint": {
            "outputs": []
        },
        "lint:fix": {
            "outputs": []
        },
        "typecheck": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "format": {
            "outputs": []
        },
        "format:check": {
            "outputs": []
        },
        "benchmark": {
            "cache": false,
            "outputs": []
        },
        "clean": {
            "cache": false,
            "outputs": []
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}
